{
  "name": "Opus-rest-api",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start --watch",
    "start:swc": "nest start --builder swc --type-check --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pm2:start": "node ./node_modules/pm2/bin/pm2 start ecosystem.config.js",
    "pm2:stop": "node ./node_modules/pm2/bin/pm2 stop ecosystem.config.js",
    "pm2:reload": "node ./node_modules/pm2/bin/pm2 reload ecosystem.config.js",
    "pm2:log": "node ./node_modules/pm2/bin/pm2 logs",
    "prod": "npm-run-all build pm2:stop pm2:start pm2:log",
    "prod:start": "npm-run-all pm2:stop pm2:start pm2:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@core/mongo(|/.*)$": "<rootDir>/libs/mongo/src/$1",
      "^@core/sql(|/.*)$": "<rootDir>/libs/sql/src/$1",
      "^@core/email(|/.*)$": "<rootDir>/libs/email/src/$1",
      "^@core/twilio(|/.*)$": "<rootDir>/libs/twilio/src/$1",
      "^@core/firebase(|/.*)$": "<rootDir>/libs/firebase/src/$1",
      "^@core/stripe(|/.*)$": "<rootDir>/libs/stripe/src/$1",
      "^@core/xps(|/.*)$": "<rootDir>libs/xps/src/$1",
      "^@core/tinyurl(|/.*)$": "<rootDir>libs/tinyurl/src/$1"
    },
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
